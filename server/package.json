  "scripts": {
    "dev": "bun --watch src/server.ts",
    "build": "bun build src/server.ts --outdir ./dist --target node",
    "start": "bun dist/server.js",
    "test:unit": "bun test",
    "test:load": "k6 run test/load/index.ts",
    "test": "npm run test:unit && npm run test:load",
    "coverage": "nyc npm run test:unit",
    "migrate": "npx prisma migrate dev",
    "deploy": "npx prisma migrate deploy && npx prisma generate",
    "pkg": "npm run build && pkg ."
